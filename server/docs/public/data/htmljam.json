          ]
        },
        {
          "MethodName": "Update user",
          "Synopsis": "Updates a user's info",
          "HTTPMethod": "PUT",
          "URI": "/users/:userId",
          "RequiresOAuth": "Y",
          "parameters": [
            {
              "Name": "userId",
              "Required": "Y",
              "Default": "",
              "Type": "string",
              "Description": "The userId parameter that is in the URI."
            },
            {
              "Name": "vignette_one, vignette_two, vignette_three",
              "Description": "ids of a jam",
              "Required": "N",
              "Type": "integer"
            }
          ]
        },
        {
          "MethodName": "Delete account",
          "Synopsis": "Delete a user account and all its data",
          "HTTPMethod": "DELETE",
          "URI": "/users/:userId",
          "RequiresOAuth": "Y",
          "parameters": [
            {
              "Name": "userId",
              "Required": "Y",
              "Default": "",
              "Type": "string",
              "Description": "The userId parameter that is in the URI."
            }
          ]
        }
      ]
    },
