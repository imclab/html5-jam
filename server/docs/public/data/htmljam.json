{
  "endpoints": [
    {
      "name": "Jams",
      "methods": [
        {
          "MethodName": "Get jam feeds",
          "Synopsis": "Returns a feed of the latest jams",
          "HTTPMethod": "GET",
          "URI": "/feeds",
          "RequiresOAuth": "Y",
          "parameters": [
            {
              "Name": "page",
              "Description": "page number",
              "Required": "N",
              "Default": "1",
              "Type": "integer"
            },
            {
              "Name": "pagination",
              "Description": "Number of results per page",
              "Required": "N",
              "Default": "20",
              "Type": "integer"
            }
          ]
        },
        {
          "MethodName": "Get jam details",
          "Synopsis": "Returns details of a jam",
          "HTTPMethod": "GET",
          "URI": "/jams/:jamId",
          "RequiresOAuth": "Y",
          "parameters": [
              {
                  "Name": "jamId",
                  "Required": "Y",
                  "Default": "",
                  "Type": "string",
                  "Description": "The jamId parameter that is in the URI."
              }
          ]
        },
        {
          "MethodName": "Create new jam",
          "Synopsis": "Creates a new jam",
          "HTTPMethod": "POST",
          "URI": "/jams",
          "RequiresOAuth": "Y",
          "parameters": [
            {
              "Name": "name",
              "Description": "name of the jam",
              "Required": "Y",
              "Type": "string"
            },
            {
              "Name": "description",
              "Description": "description of a jam",
              "Required": "N",
              "Type": "string"
            },
            {
              "Name": "privacy",
              "Description": "0 = public, 1 = private",
              "Required": "N",
              "Default": "0",
              "Type": "integer"
            }
          ]
        },
        {
          "MethodName": "Update jam",
          "Synopsis": "Update a jam info",
          "HTTPMethod": "PUT",
          "URI": "/jams/:jamId",
          "RequiresOAuth": "Y",
          "parameters": [
            {
              "Name": "jamId",
              "Required": "Y",
              "Default": "",
              "Type": "string",
              "Description": "The jamId parameter that is in the URI."
            },
            {
              "Name": "name",
              "Required": "N",
              "Type": "string"
            },
            {
              "Name": "description",
              "Required": "N",
              "Type": "string"
            },
            {
              "Name": "privacy",
              "Required": "N",
              "Type": "integer"
            }
          ]
        },
        {
          "MethodName": "Delete jam",
          "Synopsis": "Delete a jam",
          "HTTPMethod": "DELETE",
          "URI": "/jams/:jamId",
          "RequiresOAuth": "Y",
          "parameters": [
            {
              "Name": "jamId",
              "Required": "Y",
              "Default": "",
              "Type": "string",
              "Description": "The jamId parameter that is in the URI."
            }
          ]
        }
      ]
    },
    {
      "name": "Authentication",
      "methods": [
        {
          "MethodName": "Am I logged in ?",
          "Synopsis": "Checks if user is logged in and returns its account information",
          "HTTPMethod": "GET",
          "URI": "/me",
          "RequiresOAuth": "Y"
        },
        {
          "MethodName": "Login",
          "Synopsis": "Opens Facebook login window, then redirects the user with the token",
          "HTTPMethod": "GET",
          "URI": "/auth/facebook",
          "RequiresOAuth": "N"
        }
      ]
    },
    {
      "name": "Users",
      "methods": [
        {
          "MethodName": "Get a user's profile",
          "Synopsis": "Returns a user's profile",
          "HTTPMethod": "GET",
          "URI": "/users/:userId",
          "RequiresOAuth": "Y",
          "parameters": [
            {
              "Name": "userId",
              "Required": "Y",
              "Default": "",
